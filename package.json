{
  "name": "skyward-manager",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn lint && tsc",
    "watch": "tsc-watch --onSuccess \"yarn watch:start\"",
    "watch:start": "yarn lint && yarn start",
    "start": "node --import=extensionless/register .",
    "dev": "yarn build && yarn start",
    "lint": "npx eslint --fix"
  },
  "eslintConfig": {
    "extends": "@sapphire"
  },
  "type": "module",
  "devDependencies": {
    "@sapphire/eslint-config": "^5.0.1",
    "@sapphire/ts-config": "^4.0.1",
    "@types/node": "^20.4.10",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "ccisd-skyward": "^0.8.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "extensionless": "^1.5.6",
    "undici": "^6.3.0"
  },
  "engines": {
    "node": "21.x"
  }
}
